<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:translationY="4dp">

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/gris"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="20dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="17dp"
                android:layout_marginEnd="17dp"
                android:background="@color/blanco"
                android:orientation="horizontal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <com.jws.jwsapi.core.views.ScrollViewScroll
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:id="@+id/loadingPanelassa2"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            tools:layout_editor_absoluteY="59dp">

                            <LinearLayout
                                android:id="@+id/layoutwifilist"
                                android:layout_width="match_parent"
                                android:layout_height="454dp"
                                android:background="@color/blanco"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/tvSSIconnected"
                                    android:layout_width="match_parent"
                                    android:layout_height="86dp"
                                    android:background="@drawable/stylekey5"
                                    android:gravity="center"
                                    android:textColor="#26FF00"
                                    android:textSize="20sp" />

                                <ListView
                                    android:id="@+id/wifiList"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:background="@drawable/banner_menu_"
                                    android:fadeScrollbars="false"
                                    android:scrollbarSize="10dp"
                                    android:scrollbarThumbVertical="@color/colorPrimary" />


                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/tableLayoutwifi"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@color/blanco"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="20dp"
                                    android:layout_height="match_parent"
                                    android:orientation="horizontal"></LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginEnd="20dp"
                                    android:orientation="vertical">

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="44dp"
                                        android:orientation="horizontal"></LinearLayout>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="65dp"
                                        android:layout_marginTop="10dp"
                                        android:orientation="horizontal">

                                        <TextView
                                            android:id="@+id/textView9"
                                            android:layout_width="1395dp"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:fontFamily="sans-serif-black"
                                            android:text="Wifi Habilitado"
                                            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                            android:textColor="#000000"
                                            android:textSize="30sp" />

                                        <RadioGroup
                                            android:id="@+id/toggle"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:layout_weight="1"
                                            android:background="@drawable/stylekey10000"
                                            android:checkedButton="@+id/btON"
                                            android:orientation="horizontal">

                                            <RadioButton
                                                android:id="@+id/btOFF"
                                                android:layout_width="0dp"
                                                android:layout_height="match_parent"
                                                android:layout_marginLeft="1dp"
                                                android:layout_marginTop="1dp"
                                                android:layout_marginBottom="1dp"
                                                android:layout_weight="1"
                                                android:background="@drawable/togglewidgetoff"
                                                android:button="@null"
                                                android:gravity="center"
                                                android:text="OFF"
                                                android:textColor="@color/negro"
                                                android:textSize="24sp" />

                                            <RadioButton
                                                android:id="@+id/btON"
                                                android:layout_width="0dp"
                                                android:layout_height="match_parent"
                                                android:layout_marginTop="1dp"
                                                android:layout_marginRight="1dp"
                                                android:layout_marginBottom="1dp"
                                                android:layout_weight="1"
                                                android:background="@drawable/togglewidgetbackground"
                                                android:button="@null"
                                                android:gravity="center"
                                                android:text="ON"
                                                android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                                android:textColor="@color/blanco"
                                                android:textSize="24sp" />
                                        </RadioGroup>

                                    </LinearLayout>

                                    <LinearLayout
                                        android:id="@+id/ln6"
                                        android:layout_width="match_parent"
                                        android:layout_height="65dp"
                                        android:layout_marginTop="50dp"
                                        android:orientation="horizontal">

                                        <TextView
                                            android:id="@+id/tvtitulo6"
                                            android:layout_width="653dp"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:fontFamily="sans-serif-black"
                                            android:text="Direccion MAC"
                                            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                            android:textColor="#000000"
                                            android:textSize="30sp" />

                                        <TextView
                                            android:id="@+id/tvMAC"
                                            android:layout_width="180dp"
                                            android:layout_height="match_parent"
                                            android:layout_weight="1"
                                            android:gravity="center"
                                            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                            android:textColor="#000000"
                                            android:textSize="24sp" />

                                    </LinearLayout>

                                    <LinearLayout
                                        android:id="@+id/ln5"
                                        android:layout_width="match_parent"
                                        android:layout_height="65dp"
                                        android:layout_marginTop="50dp"
                                        android:orientation="horizontal">

                                        <TextView
                                            android:id="@+id/textsadsadView52"
                                            android:layout_width="653dp"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:fontFamily="sans-serif-black"
                                            android:text="Red Wifi"
                                            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                            android:textColor="#000000"
                                            android:textSize="30sp" />

                                        <TextView
                                            android:id="@+id/tvSSIRED"
                                            android:layout_width="180dp"
                                            android:layout_height="match_parent"
                                            android:layout_weight="1"
                                            android:gravity="center"
                                            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                            android:textColor="#000000"
                                            android:textSize="24sp" />

                                    </LinearLayout>


                                    <LinearLayout
                                        android:id="@+id/ln4"
                                        android:layout_width="match_parent"
                                        android:layout_height="65dp"
                                        android:layout_marginTop="50dp"
                                        android:orientation="horizontal">

                                        <TextView
                                            android:id="@+id/textVddsiew52"
                                            android:layout_width="836dp"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:fontFamily="sans-serif-black"
                                            android:text="Lista de redes"
                                            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                            android:textColor="#000000"
                                            android:textSize="30sp" />

                                        <RelativeLayout
                                            android:layout_width="250dp"
                                            android:layout_height="wrap_content"
                                            android:layout_alignParentRight="true"
                                            android:layout_marginBottom="5dp"
                                            android:orientation="horizontal">

                                            <TextView
                                                android:id="@+id/tvred"
                                                android:layout_width="match_parent"
                                                android:layout_height="match_parent"
                                                android:background="@drawable/spinnerbackground"
                                                android:fontFamily="sans-serif-medium"
                                                android:gravity="center"
                                                android:text="REDES"
                                                android:textColor="#000000"
                                                android:textSize="20sp" />


                                            <ImageView
                                                android:layout_width="wrap_content"
                                                android:layout_height="34dp"
                                                android:layout_alignParentTop="true"
                                                android:layout_alignParentRight="true"
                                                android:layout_gravity="center"
                                                android:layout_marginTop="9dp"
                                                android:layout_marginRight="14dp"
                                                android:src="@drawable/flecha_arriba" />
                                        </RelativeLayout>
                                    </LinearLayout>


                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="50dp"
                                        android:layout_marginTop="10dp"
                                        android:orientation="horizontal"
                                        android:visibility="gone">

                                        <TextView
                                            android:id="@+id/textdsdsView52"
                                            android:layout_width="653dp"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:fontFamily="sans-serif-black"
                                            android:text="IP"
                                            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                            android:textColor="#000000"
                                            android:textSize="30sp" />

                                        <TextView
                                            android:id="@+id/tvIP"
                                            android:layout_width="180dp"
                                            android:layout_height="match_parent"
                                            android:layout_weight="1"
                                            android:background="@drawable/fondolineainferiornegratranslucido"
                                            android:gravity="center"
                                            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                            android:textColor="#000000"
                                            android:textSize="24sp" />

                                    </LinearLayout>


                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="50dp"
                                        android:layout_marginTop="10dp"
                                        android:orientation="horizontal"
                                        android:visibility="gone">

                                        <TextView
                                            android:id="@+id/textVidsddddew52"
                                            android:layout_width="147dp"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:fontFamily="sans-serif-black"
                                            android:text="Ethernet habilitado"
                                            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                            android:textColor="#000000"
                                            android:textSize="30sp" />

                                        <RelativeLayout
                                            android:layout_width="250dp"
                                            android:layout_height="match_parent"
                                            android:orientation="horizontal">

                                            <Spinner
                                                android:id="@+id/sethernetsino"
                                                android:layout_width="match_parent"
                                                android:layout_height="match_parent"
                                                android:background="@drawable/boton"
                                                android:entries="@array/Unitsino"
                                                android:textAlignment="center" />

                                            <ImageView
                                                android:layout_width="wrap_content"
                                                android:layout_height="35dp"
                                                android:layout_alignParentTop="true"
                                                android:layout_alignParentRight="true"
                                                android:layout_gravity="center"
                                                android:layout_marginTop="17dp"
                                                android:layout_marginRight="21dp"
                                                android:src="@drawable/flecha_arriba" />

                                        </RelativeLayout>

                                    </LinearLayout>

                                </LinearLayout>

                            </LinearLayout>

                        </RelativeLayout>

                    </LinearLayout>
                </com.jws.jwsapi.core.views.ScrollViewScroll>

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <RelativeLayout
        android:id="@+id/loadingPanel2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        tools:layout_editor_absoluteY="59dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>