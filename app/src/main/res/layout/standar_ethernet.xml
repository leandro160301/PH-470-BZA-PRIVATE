<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:translationY="4dp">

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/gris"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="20dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="17dp"
                android:layout_marginEnd="17dp"
                android:background="@color/blanco"
                android:orientation="horizontal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <com.jws.jwsapi.core.views.ScrollViewScroll
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <LinearLayout
                            android:id="@+id/tableLayoutwifi"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="20dp"
                                android:layout_height="match_parent"
                                android:orientation="horizontal"></LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginEnd="20dp"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="80dp"
                                    android:layout_marginTop="10dp"
                                    android:layout_weight="1"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/textView9"
                                        android:layout_width="1470dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:fontFamily="sans-serif-black"
                                        android:text="Ethernet Habilitado"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                        android:textColor="#000000"
                                        android:textSize="30sp" />

                                    <RadioGroup
                                        android:id="@+id/toggle"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:background="@drawable/stylekey10000"
                                        android:checkedButton="@+id/btON"
                                        android:orientation="horizontal">

                                        <RadioButton
                                            android:id="@+id/btOFF"
                                            android:layout_width="0dp"
                                            android:layout_height="match_parent"
                                            android:layout_marginLeft="1dp"
                                            android:layout_marginTop="1dp"
                                            android:layout_marginBottom="1dp"
                                            android:layout_weight="1"
                                            android:background="@drawable/togglewidgetoff"
                                            android:button="@null"
                                            android:gravity="center"
                                            android:text="OFF"
                                            android:textColor="@color/negro"
                                            android:textSize="24sp" />

                                        <RadioButton
                                            android:id="@+id/btON"
                                            android:layout_width="0dp"
                                            android:layout_height="match_parent"
                                            android:layout_marginTop="1dp"
                                            android:layout_marginRight="1dp"
                                            android:layout_marginBottom="1dp"
                                            android:layout_weight="1"
                                            android:background="@drawable/togglewidgetbackground"
                                            android:button="@null"
                                            android:gravity="center"
                                            android:text="ON"
                                            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                            android:textColor="@color/blanco"
                                            android:textSize="24sp" />
                                    </RadioGroup>

                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/ln0"
                                    android:layout_width="match_parent"
                                    android:layout_height="80dp"
                                    android:layout_marginTop="10dp"
                                    android:layout_weight="1"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tvtitulo1"
                                        android:layout_width="1470dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:fontFamily="sans-serif-black"
                                        android:text="DHCP"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                        android:textColor="#000000"
                                        android:textSize="30sp" />

                                    <RadioGroup
                                        android:id="@+id/toggle2"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:background="@drawable/stylekey10000"
                                        android:checkedButton="@+id/btON2"
                                        android:orientation="horizontal">

                                        <RadioButton
                                            android:id="@+id/btOFF2"
                                            android:layout_width="0dp"
                                            android:layout_height="match_parent"
                                            android:layout_marginLeft="1dp"
                                            android:layout_marginTop="1dp"
                                            android:layout_marginBottom="1dp"
                                            android:layout_weight="1"
                                            android:background="@drawable/togglewidgetoff"
                                            android:button="@null"
                                            android:gravity="center"
                                            android:text="OFF"
                                            android:textColor="@color/negro"
                                            android:textSize="24sp" />

                                        <RadioButton
                                            android:id="@+id/btON2"
                                            android:layout_width="0dp"
                                            android:layout_height="match_parent"
                                            android:layout_marginTop="1dp"
                                            android:layout_marginRight="1dp"
                                            android:layout_marginBottom="1dp"
                                            android:layout_weight="1"
                                            android:background="@drawable/togglewidgetbackground"
                                            android:button="@null"
                                            android:gravity="center"
                                            android:text="ON"
                                            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                            android:textColor="@color/blanco"
                                            android:textSize="24sp" />
                                    </RadioGroup>

                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/ln1"
                                    android:layout_width="match_parent"
                                    android:layout_height="80dp"
                                    android:layout_marginTop="10dp"
                                    android:layout_weight="1"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tvtitulo2"
                                        android:layout_width="653dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:fontFamily="sans-serif-black"
                                        android:text="Direccion Ip"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                        android:textColor="#000000"
                                        android:textSize="30sp" />

                                    <TextView
                                        android:id="@+id/tvip"
                                        android:layout_width="180dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:background="@drawable/stylekeycor6ceropaddin"
                                        android:gravity="center"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                        android:textColor="#000000"
                                        android:textSize="24sp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/ln2"
                                    android:layout_width="match_parent"
                                    android:layout_height="80dp"
                                    android:layout_marginTop="10dp"
                                    android:layout_weight="1"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tvtitulo3"
                                        android:layout_width="653dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:fontFamily="sans-serif-black"
                                        android:text="Subnet"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                        android:textColor="#000000"
                                        android:textSize="30sp" />

                                    <TextView
                                        android:id="@+id/tvsubnet"
                                        android:layout_width="180dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:background="@drawable/stylekeycor6ceropaddin"
                                        android:gravity="center"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                        android:textColor="#000000"
                                        android:textSize="24sp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/ln3"
                                    android:layout_width="match_parent"
                                    android:layout_height="80dp"
                                    android:layout_marginTop="10dp"
                                    android:layout_weight="1"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tvtitulo4"
                                        android:layout_width="653dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:fontFamily="sans-serif-black"
                                        android:text="Gateway"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                        android:textColor="#000000"
                                        android:textSize="30sp" />

                                    <TextView
                                        android:id="@+id/tvgateway"
                                        android:layout_width="180dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:background="@drawable/stylekeycor6ceropaddin"
                                        android:gravity="center"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                        android:textColor="#000000"
                                        android:textSize="24sp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/ln4"
                                    android:layout_width="match_parent"
                                    android:layout_height="80dp"
                                    android:layout_marginTop="10dp"
                                    android:layout_weight="1"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tvtitulo5"
                                        android:layout_width="653dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:fontFamily="sans-serif-black"
                                        android:text="DNS 1"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                        android:textColor="#000000"
                                        android:textSize="30sp" />

                                    <TextView
                                        android:id="@+id/tvdns1"
                                        android:layout_width="180dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:background="@drawable/stylekeycor6ceropaddin"
                                        android:gravity="center"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                        android:textColor="#000000"
                                        android:textSize="24sp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/ln5"
                                    android:layout_width="match_parent"
                                    android:layout_height="80dp"
                                    android:layout_marginTop="10dp"
                                    android:layout_weight="1"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tvtitulo6"
                                        android:layout_width="653dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:fontFamily="sans-serif-black"
                                        android:text="DNS 2"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                        android:textColor="#000000"
                                        android:textSize="30sp" />

                                    <TextView
                                        android:id="@+id/tvdns2"
                                        android:layout_width="180dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:background="@drawable/stylekeycor6ceropaddin"
                                        android:gravity="center"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                        android:textColor="#000000"
                                        android:textSize="24sp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/ln6"
                                    android:layout_width="match_parent"
                                    android:layout_height="80dp"
                                    android:layout_marginTop="10dp"
                                    android:layout_marginBottom="5dp"
                                    android:layout_weight="1"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:layout_width="653dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:fontFamily="sans-serif-black"
                                        android:text="Direccion MAC"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                        android:textColor="#000000"
                                        android:textSize="30sp" />

                                    <TextView
                                        android:id="@+id/tvMAC"
                                        android:layout_width="180dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                        android:textColor="#000000"
                                        android:textSize="24sp" />

                                </LinearLayout>


                            </LinearLayout>

                        </LinearLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            tools:layout_editor_absoluteY="59dp">

                        </RelativeLayout>

                    </LinearLayout>
                </com.jws.jwsapi.core.views.ScrollViewScroll>

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        tools:layout_editor_absoluteY="59dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>