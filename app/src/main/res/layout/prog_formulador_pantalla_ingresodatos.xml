                            android:layout_height="match_parent"
                            android:orientation="horizontal"></LinearLayout>

                        <com.jws.jwsapi.base.customviews.ScrollViewScroll
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="20dp"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="11dp"
                                    android:orientation="horizontal"></LinearLayout>

                                <LinearLayout
                                    android:id="@+id/ln_lote"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="20dp"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tvwqwq_1"
                                        android:layout_width="1067dp"
                                        android:layout_height="53dp"
                                        android:layout_weight="1"
                                        android:fontFamily="sans-serif-black"
                                        android:text="Lote"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                        android:textColor="#000000"
                                        android:textSize="30sp" />

                                    <TextView
                                        android:id="@+id/tv_lote"
                                        android:layout_width="1574dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:background="@drawable/campollenarclickeableceropadding_selector"
                                        android:gravity="center"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                        android:textColor="#000000"
                                        android:textSize="36sp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="20dp"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tsasav_1"
                                        android:layout_width="1067dp"
                                        android:layout_height="53dp"
                                        android:layout_weight="1"
                                        android:fontFamily="sans-serif-black"
                                        android:text="Vencimiento"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                        android:textColor="#000000"
                                        android:textSize="30sp" />

                                    <TextView
                                        android:id="@+id/tv_vencimiento"
                                        android:layout_width="1574dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:background="@drawable/campollenarclickeableceropadding_selector"
                                        android:gravity="center"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
