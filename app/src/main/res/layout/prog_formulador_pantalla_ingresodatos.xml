                                        android:gravity="center"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                        android:textColor="#000000"
                                        android:textSize="36sp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/ln_campo2"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="20dp"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tv_2"
                                        android:layout_width="1067dp"
                                        android:layout_height="53dp"
                                        android:layout_weight="1"
                                        android:fontFamily="sans-serif-black"
                                        android:text="Campo 2"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                        android:textColor="#000000"
                                        android:textSize="30sp" />

                                    <TextView
                                        android:id="@+id/tv_campo2"
                                        android:layout_width="1574dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:background="@drawable/campollenarclickeableceropadding_selector"
                                        android:gravity="center"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                        android:textColor="#000000"
                                        android:textSize="36sp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/ln_campo3"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
